{
	"name": "HSTS",
	"version": "1.1.1",
	"author": "Seb35",
	"url": "https://www.mediawiki.org/wiki/Extension:HSTS",
	"descriptionmsg": "hsts-desc",
	"license-name": "WTFPL",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.35.0"
	},
	"DefaultUserOptions": {
		"hsts": 0
	},
	"MessagesDirs": {
		"HSTS": [
			"i18n"
		]
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\HSTS\\": "includes/"
	},
	"Hooks": {
		"GetPreferences": "MediaWiki\\Extension\\HSTS\\Hooks::getPreferences",
		"GetBetaFeaturePreferences": "MediaWiki\\Extension\\HSTS\\Hooks::getBetaFeaturePreferences",
		"BeforePageDisplay": "MediaWiki\\Extension\\HSTS\\Hooks::addHeader"
	},
	"config": {
		"HSTSForAnons": false,
		"HSTSForUsers": false,
		"HSTSMaxAge": 2592000,
		"HSTSIncludeSubdomains": false,
		"HSTSBetaFeature": false
	},
	"manifest_version": 1
}
